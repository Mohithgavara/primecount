import java.util.*;
class primecount
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int i,j,t,n,c=0;
System.out.println("enter number");
n=sc.nextInt();
for(i=2;i<=n;i++)
{
t=1;
for(j=2;j<i;j++)
{
if(i%j==0)
{
t=0;
break;
}
}
if(t==1)
c++;
}
System.out.println(c);
}
}
